#!/home/juan/Documentos/python/crud/proyecto_platzi/Juan_ventas/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'jv','console_scripts','jv'
__requires__ = 'jv'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jv', 'console_scripts', 'jv')()
    )
